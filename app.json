{
  "expo": {
    "name": "property-suchna",
    "slug": "property-suchna",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/test/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/test/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "property-suchna-ios"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/test/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.santosh_singh.propertysuchna",
      "enableDangerousExperimentalLeanBuilds": true
    },
    "web": {
      "favicon": "./assets/test/favicon.png"
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "afc47845-44c1-4527-8697-cd21e2d0d3f1"
      }
    }
  }
}